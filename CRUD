def agregar_alumno(matrizalumnos,legajo,nombre,apellido):
    for alumno in matrizalumnos[1:]:
        if alumno[0]=legajo:
            print('El alumno ya existe')
    matrizalumnos.append([Legajo,nombre,apellido])
    print(f'Alumno con Legajo {legajo} creado exitosamente.')

def leer_alumno(matrizalumnos, legajos):
    for alumno in matrizalumnos [1:]:
        if alumno[0]==legajo:
            return alumno
    print(f"El numero de legajo {legajo} no existe")

